Resources:
    AWSEBRDSDatabase:
        Type: 'AWS::RDS::DBInstance'
        Properties:
            AllocatedStorage: '5'
            DBInstanceClass: 'db.t2.micro'
            Engine: 'MySQL'
            EngineVersion: '9.3'
            DBName: 'ehealth_db'
            MasterUsername: 'ehealth'
            MasterUserPassword:
            Ref: 'DBPassword'